<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
  <comment>File locations for some of the LemPORT classes</comment>
  <!-- Number declensions -->
  <entry key="numberDeclensions">resources/replacements/numberdeclensions.xml</entry>
  <!-- Gender declensions and gender specific nouns -->
  <entry key="genderDeclensions">resources/replacements/genderdeclensions.xml</entry>
  <entry key="genderNouns">resources/replacements/gendernouns.xml</entry>
  <!-- Verb conjugations, lexemes and declensions -->
  <entry key="irregularVerbConjugations">resources/replacements/irregularverbconjugations.xml</entry>
  <entry key="regularVerbLexemes">resources/replacements/regularverblexemes.xml</entry>
  <entry key="regularVerbDeclensions">resources/replacements/regularverbdeclensions.xml</entry>
  <!-- Augmentative, dinimutive and superlative declensions -->
  <entry key="augmentativeDeclensions">resources/replacements/augmentativedeclensions.xml</entry>
  <entry key="diminutiveDeclensions">resources/replacements/diminutivedeclensions.xml</entry>
  <entry key="superlativeDeclensions">resources/replacements/superlativedeclensions.xml</entry>
  <!-- Adverb (manner) declensions -->
  <entry key="adverbDeclensions">resources/replacements/adverbdeclensions.xml</entry>
  <!-- Dictionary and lexicon data -->
  <entry key="dictionary">resources/label/Label-Delaf_pt_v4_1.dic</entry>
  <entry key="customDictionary">resources/dictionary/custom.dic</entry>
  <entry key="dictionaryExclusions">DET|PRO</entry>
  <entry key="wordRanking">resources/acdc/lemas.total.txt</entry>
  <entry key="lexConversions">PRP:PREP;PRON:PRO;IN:INTERJ;ART:DET</entry>
</properties>
